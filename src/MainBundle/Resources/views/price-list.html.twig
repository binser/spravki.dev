{% extends 'MainBundle::base.html.twig' %}

{% block title %}{% endblock %}
{% block description %}{% endblock %}
{% block keywords %}{% endblock %}

{% block content %}
    <section id="price-table">
        <div class="container">
            <div class="table">
                <h1>Прайс-лист</h1>
                <h3>Для работнинков</h3>
                <table>
                    {% for item in pricetable %}

                        <tr>
                            <td><a href="#">{{ item.name }}</a></td>
                            <td>{{ item.price }}</td>
                        </tr>
                    {% endfor %}
                </table>
            </div>
        </div>
    </section>
{% endblock %}